    "@mui/material": "^5.11.8",
    "@next/font": "13.1.6",
    "eslint": "8.33.0",
    "eslint-config-next": "^14.0.4",
    "next": "^14.0.4",
    "next-absolute-url": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-duration": "^1.1.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
